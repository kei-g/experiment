{
  "compilerOptions": {
    "alwaysStrict": true,
    "charset": "utf8",
    "declaration": true,
    "declarationDir": ".",
    "downlevelIteration": true,
    "emitBOM": false,
    "emitDeclarationOnly": true,
    "lib": [
      "ES2015",
    ],
    "module": "CommonJS",
    "moduleResolution": "node",
    "newLine": "lf",
    "outDir": ".",
    "pretty": true,
    "removeComments": true,
    "strict": true,
    "strictNullChecks": false,
    "target": "ES6",
    "typeRoots": [
      "node_modules/@types"
    ]
  },
  "exclude": [
  ],
  "include": [
    "src",
  ],
}
